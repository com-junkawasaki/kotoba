{
  server: {
    host: "127.0.0.1",
    port: 8080,
    max_connections: 1000,
    timeout_ms: 30000,
    graphql_enabled: true
  },
  routes: [
    {
      method: "GET",
      path: "/api/health",
      handler: "health_check",
      authRequired: false,
      corsEnabled: true
    }
  ],
  middlewares: [
    {
      name: "cors",
      order: 1,
      function: "cors_middleware"
    }
  ]
}
