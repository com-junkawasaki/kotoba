// Test Kotoba file for formatter
graph example   {
    node user{
        name: "Alice"
        age:25
    }

    node profile{
        email:"alice@example.com"
        active: true
    }

    edge connects user->profile
}

query find_user{
    match (u:user)-[:connects]->(p:profile)
    where u.age > 20
    return u.name, p.email
}

// Another query
query active_users   {
    match (u:user{active:true})
    return u.name
}

fn greet(name){
    return "Hello, " + name + "!"
}
