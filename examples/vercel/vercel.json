{
  "version": 2,
  "builds": [
    {
      "src": "graphql.rs",
      "use": "@vercel/rust",
      "config": {
        "features": ["vercel"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/graphql.rs"
    },
    {
      "src": "/api/graphql/playground",
      "dest": "/graphql.rs"
    },
    {
      "src": "/api/health",
      "dest": "/graphql.rs"
    }
  ],
  "functions": {
    "graphql.rs": {
      "runtime": "@vercel/rust@4.0.0"
    }
  },
  "env": {
    "RUST_LOG": "info",
    "REDIS_URL": "@redis-url"
  }
}
