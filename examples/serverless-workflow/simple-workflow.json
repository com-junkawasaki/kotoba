{
    "dsl": "1.0.0",
    "namespace": "examples",
    "name": "simple-workflow",
    "version": "1.0.0",
    "description": "A simple workflow that sets variables and waits",
    "do": [
        {
            "type": "set",
            "name": "set-message",
            "variables": {
                "message": "Hello, World!",
                "timestamp": "2024-01-01T00:00:00Z"
            }
        },
        {
            "type": "wait",
            "name": "wait-5-seconds",
            "wait": {
                "seconds": 5
            }
        }
    ]
}